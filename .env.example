# PulseGuard Expo App Environment Variables

# Expo Project Configuration
EXPO_PUBLIC_PROJECT_ID=your-expo-project-id-here

# Backend API Configuration
EXPO_PUBLIC_API_URL=https://app.pulseguard.nl
EXPO_PUBLIC_API_TIMEOUT=10000

# App Configuration
EXPO_PUBLIC_APP_NAME="PulseGuard Kiosk"
EXPO_PUBLIC_APP_VERSION=1.0.0

# Notification Configuration
EXPO_PUBLIC_NOTIFICATION_SOUND=default
EXPO_PUBLIC_NOTIFICATION_VIBRATE=true

# Development Configuration
EXPO_PUBLIC_DEV_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info

# Kiosk Configuration
EXPO_PUBLIC_KIOSK_URL=https://app.pulseguard.nl
EXPO_PUBLIC_KEEP_AWAKE=true
EXPO_PUBLIC_FULLSCREEN=true

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_ENABLED=false 